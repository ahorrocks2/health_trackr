<%= render 'layouts/errors', :object => @food %>

<%= form_for [@user, @food], :html => {:class => "form-vertical center"} do |f| %>
  <%= f.text_field :name, :placeholder => "Name", class: 'form-control' %>
  <%= f.text_field :cal_consumed, :placeholder => "Calories Consumed", class: 'form-control' %>
  <%= f.submit "Add Food" %>
<% end %>
